{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CargoStateDb": "Data Source=.;Initial Catalog=CargoStateMachineDB;Integrated Security=True;",
    "SagaConnectionString": "",
    "CargoConnectionString": "",
    "DeliveryConnectionString": "",
    "Monitoring": "Data Source=.;Initial Catalog=CargoMonitoring;Integrated Security=True;"
  },
  "MessageBroker": {
    "Provider": "RabbitMQ",
    "RabbitMQ": {
      "Name": "MainHost",
      "HostName": "localhost",
      "UserName": "guest",
      "Password": "guest",
      "RetryCount": 2,
      "RetryTimeInterval": 1000,
      "VirtualHost": "/",
      "PrefetchCount": 1,
      "TrackingPeriod": 1,
      "TripThreshold": 50,
      "ActiveThreshold": 50,
      "ResetInterval": 1
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092",
      "Topics": {
        "FileUploadedEvent": "classifiedadds_fileuploaded",
        "FileDeletedEvent": "classifiedadds_filedeleted",
        "EmailMessageCreatedEvent": "classifiedadds_emailcreated",
        "SmsMessageCreatedEvent": "classifiedadds_smscreated"
      },
      "GroupId": "order"
    }
  },
  "Services": {
    "Saga": "https://localhost:5010",
    "Cargo": {
      "Grpc": "https://localhost:5011"
    },
    "Route": {
      "Grpc": "https://localhost:5012"
    },
    "Delivery": {
      "Grpc": "https://localhost:5013"
    },
    "Payment": {
      "Grpc": "https://localhost:5014"
    }
  },
  "Caching": {
    "Distributed": {
      "Provider": "Redis",
      "Redis": {
        "Configuration": "127.0.0.1:6379",
        "InstanceName": "order-"
      },
      "SqlServer": {
        "ConnectionString": "",
        "SchemaName": "",
        "TableName": ""
      }
    }
  },
  "AllowedHosts": "*"
}