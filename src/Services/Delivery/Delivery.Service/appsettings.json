{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ConnectionString": "Server=DESKTOP-CHB8USB;Database=CargoDelivery;Trusted_Connection=True;"
  },
  "MessageBroker": {
    "Provider": "RabbitMQ",
    "RabbitMQ": {
      "Name": "MainHost",
      "HostName": "localhost",
      "UserName": "guest",
      "Password": "guest",
      "RetryCount": 2,
      "RetryTimeInterval": 1000,
      "VirtualHost": "/",
      "PrefetchCount": 1,
      "TrackingPeriod": 1,
      "TripThreshold": 50,
      "ActiveThreshold": 50,
      "ResetInterval": 1
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092",
      "Topics": {
        "FileUploadedEvent": "classifiedadds_fileuploaded",
        "FileDeletedEvent": "classifiedadds_filedeleted",
        "EmailMessageCreatedEvent": "classifiedadds_emailcreated",
        "SmsMessageCreatedEvent": "classifiedadds_smscreated"
      },
      "GroupId": "delivery"
    }
  },
  "AllowedHosts": "*"
}