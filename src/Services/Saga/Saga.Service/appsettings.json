{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CargoStateDb": "Data Source=.;Initial Catalog=CargoStateMachineDB;Integrated Security=True;"
  },
  "MessageBroker": {
    "Provider": "Kafka",
    "RabbitMQ": {
      "Name": "MainHost",
      "HostName": "localhost",
      "UserName": "guest",
      "Password": "guest",
      "RetryCount": 2,
      "RetryTimeInterval": 1000,
      "VirtualHost": "/",
      "PrefetchCount": 1,
      "TrackingPeriod": 1,
      "TripThreshold": 50,
      "ActiveThreshold": 50,
      "ResetInterval": 1
    },
    "Kafka": {
      "BootstrapServers": "localhost:9092",
      "SchemaRegistryUrl": "http://localhost:8081",
      "SecurityProtocol": "",
      "Username": null,
      "Password": null,
      "SaslMechanism": null,
      "Topics": {
        "FileUploadedEvent": "classifiedadds_fileuploaded",
        "FileDeletedEvent": "classifiedadds_filedeleted",
        "EmailMessageCreatedEvent": "classifiedadds_emailcreated",
        "SmsMessageCreatedEvent": "classifiedadds_smscreated"
      },
      "GroupId": "cargo"
    }
  },
  "AllowedHosts": "*"
}
